{
    "name": "icecave/parity",
    "description": "A comparator library for PHP.",
    "keywords": ["compare", "comparison", "equal", "equality", "less", "greater", "sort", "sorting"],
    "homepage": "https://github.com/IcecaveStudios/parity",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.9",
        "icecave/archer": "~0.3"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Parity": "lib",
            "Icecave\\Parity\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\Parity\\TypeCheck",
            "use-native-callable": false
        }
    }
}
